	<!-- Begin RewriteRules -->
	<rewrite trailingSlash="remove" />

	<!-- <rewrite match="^/old-url/(\w+)/$" substitute="/new_url/$1/" redirect="301" 
		/> -->
	<rewrite match="/metadataProfile/(.+)" substitute="/profile/$1"
		redirect="301" />
	<!-- Begin UrlMappings -->

	<url-mapping id="home">
		<pattern value="/" />
		<view-id>/jsf/MaintenancePage.xhtml</view-id>
	</url-mapping>
	<url-mapping id="albums">
